Q2)shell script

PROGRAM:

echo -e "1) for rectangle \n 2) for triangle \n 3) for circle "
echo "enter your choice"
read ch
case $ch in
1)echo "enter the lenght and breadth of reactangle"
read l
read b
((a=l*b))
echo "area of triangle is "$a ;;
2) echo "enter the length and height of triangle"
read l
read b
((a=(l*b)/2))
echo "area of triangle =" $a ;;
3) echo "enter the radius of circle"
read r
((a=22*r*r))
((ar=a/7))
echo "area of circle is "$ar;;
*) echo "inalid entry" ;;

esac
 
 OUTPUT
 
